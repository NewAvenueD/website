

<ul class="main-navigation">
  <li>&nbsp;</li>
  {% for page in pages.children.visible %}
  {% set is_active = (page.active or page.activeChild) ? 'active' : '' %}
  <li class="item separator {{ is_active }}">
    <a href="{{ page.url }}">
      {{ page.menu }}
    </a>
  </li>
  {% endfor %}
</ul>
